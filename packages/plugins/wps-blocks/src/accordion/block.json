{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "wps/accordion",
	"title": "Accordion",
	"description": "List content in an accordion.",
	"category": "common",
	"icon": "editor-aligncenter",
	"textdomain": "wps-blocks",
	"supports": {
		"anchor": true,
		"html": false,
		"align": ["full", "wide"]
	},
	"keywords": ["accordion", "list"],
	"attributes": {
		"anchor": {
			"type": "string"
		},
		"align": {
			"type": "string"
		},
		"showMultiple": {
			"type": "boolean"
		},
		"openFirst": {
			"type": "boolean"
		},
		"htmlTag": {
			"type": "string"
		},
		"marginTop": {
			"type": "string"
		},
		"marginBottom": {
			"type": "string"
		}
	},
	"style": "file:../../build/accordion/style.css",
	"editorStyle": "file:../../build/accordion/editor.css",
	"editorScript": "file:../../build/accordion/index.js",
	"script": "file:../../build/accordion/frontend.js"
}
