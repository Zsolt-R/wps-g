{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "wps/icon",
	"title": "Icon block",
	"description": "Fontawesome Icon",
	"category": "common",
	"icon": "star-filled",
	"supports": {
		"anchor": true,
		"html": false,
		"align": []
	},
	"keywords": ["Icon", "Fontawesome"],
	"attributes": {
		"icon": {
			"type": "string",
			"default": "face-smile"
		},
		"textColor": {
			"type": "string"
		},
		"justification": {
			"type": "string"
		},
		"fontSize": {
			"type": "object"
		},
		"type": {
			"type": "string"
		},
		"marginTop": {
			"type": "string"
		},
		"marginBottom": {
			"type": "string"
		}
	},
	"style": "file:../../build/icon/style.css",
	"editorScript": "file:../../build/icon/index.js"
}
